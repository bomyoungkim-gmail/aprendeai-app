  "resources": [
    {
      "_id": "wrk_cornell_reader",
      "_type": "workspace",
      "name": "Cornell Reader V1 API",
      "description": "API completa do Cornell Reader backend",
      "parentId": null
    },
    {
      "_id": "env_cornell_local",
      "_type": "environment",
      "name": "Local Development",
      "data": {
        "baseUrl": "http://localhost:3000",
        "email": "maria@example.com",
        "password": "demo123",
        "token": "",
        "contentId": "",
        "fileId": "",
        "highlightId": ""
      },
      "parentId": "wrk_cornell_reader"
    },
    {
      "_id": "fld_auth",
      "_type": "request_group",
      "name": "1. Authentication",
      "parentId": "wrk_cornell_reader"
    },
    {
      "_id": "req_login",
      "_type": "request",
      "name": "Login",
      "method": "POST",
      "url": "{{ _.baseUrl }}/auth/login",
      "body": {
        "mimeType": "application/json",
        "text": "{\n  \"email\": \"{{ _.email }}\",\n  \"password\": \"{{ _.password }}\"\n}"
      },
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "authentication": {},
      "parentId": "fld_auth"
    },
    {
      "_id": "fld_contents",
      "_type": "request_group",
      "name": "2. Contents",
      "parentId": "wrk_cornell_reader"
    },
    {
      "_id": "req_list_contents",
      "_type": "request",
      "name": "List My Contents",
      "method": "GET",
      "url": "{{ _.baseUrl }}/api/contents/my-contents",
      "headers": [],
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      },
      "parentId": "fld_contents"
    },
    {
      "_id": "req_get_content",
      "_type": "request",
      "name": "Get Content by ID",
      "method": "GET",
      "url": "{{ _.baseUrl }}/api/contents/{{ _.contentId }}",
      "headers": [],
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      },
      "parentId": "fld_contents"
    },
    {
      "_id": "fld_cornell",
      "_type": "request_group",
      "name": "3. Cornell Notes",
      "parentId": "wrk_cornell_reader"
    },
    {
      "_id": "req_get_cornell",
      "_type": "request",
      "name": "Get or Create Cornell Notes",
      "method": "GET",
      "url": "{{ _.baseUrl }}/api/contents/{{ _.contentId }}/cornell",
      "headers": [],
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      },
      "parentId": "fld_cornell"
    },
    {
      "_id": "req_update_cornell",
      "_type": "request",
      "name": "Update Cornell Notes",
      "method": "PUT",
      "url": "{{ _.baseUrl }}/api/contents/{{ _.contentId }}/cornell",
      "body": {
        "mimeType": "application/json",
        "text": "{\n  \"cuesJson\": [\n    {\n      \"id\": \"cue-1\",\n      \"prompt\": \"What is the main concept?\",\n      \"linked_highlight_ids\": []\n    }\n  ],\n  \"notesJson\": [\n    {\n      \"id\": \"note-1\",\n      \"body\": \"This is the main idea.\",\n      \"linked_highlight_ids\": []\n    }\n  ],\n  \"summaryText\": \"Overall summary here.\"\n}"
      },
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      },
      "parentId": "fld_cornell"
    },
    {
      "_id": "fld_highlights",
      "_type": "request_group",
      "name": "4. Highlights",
      "parentId": "wrk_cornell_reader"
    },
    {
      "_id": "req_list_highlights",
      "_type": "request",
      "name": "List Highlights",
      "method": "GET",
      "url": "{{ _.baseUrl }}/api/contents/{{ _.contentId }}/highlights",
      "headers": [],
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      },
      "parentId": "fld_highlights"
    },
    {
      "_id": "req_create_highlight_pdf",
      "_type": "request",
      "name": "Create Highlight - PDF Text",
      "method": "POST",
      "url": "{{ _.baseUrl }}/api/contents/{{ _.contentId }}/highlights",
      "body": {
        "mimeType": "application/json",
        "text": "{\n  \"kind\": \"TEXT\",\n  \"targetType\": \"PDF\",\n  \"pageNumber\": 1,\n  \"anchorJson\": {\n    \"type\": \"PDF_TEXT\",\n    \"position\": {\n      \"boundingRect\": {\"x1\": 100, \"y1\": 200, \"x2\": 400, \"y2\": 220, \"width\": 300, \"height\": 20},\n      \"rects\": [{\"x1\": 100, \"y1\": 200, \"x2\": 400, \"y2\": 220, \"width\": 300, \"height\": 20, \"pageNumber\": 1}],\n      \"pageNumber\": 1\n    },\n    \"quote\": \"Highlighted text from PDF\"\n  },\n  \"colorKey\": \"yellow\",\n  \"commentText\": \"Important!\",\n  \"tagsJson\": [\"study\"]\n}"
      },
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      },
      "parentId": "fld_highlights"
    },
    {
      "_id": "req_create_highlight_image",
      "_type": "request",
      "name": "Create Highlight - Image Area",
      "method": "POST",
      "url": "{{ _.baseUrl }}/api/contents/{{ _.contentId }}/highlights",
      "body": {
        "mimeType": "application/json",
        "text": "{\n  \"kind\": \"AREA\",\n  \"targetType\": \"IMAGE\",\n  \"anchorJson\": {\n    \"type\": \"IMAGE_AREA\",\n    \"rect\": {\"x\": 120, \"y\": 80, \"w\": 200, \"h\": 150},\n    \"zoom\": 1,\n    \"viewport\": {\"width\": 800, \"height\": 600}\n  },\n  \"colorKey\": \"red\",\n  \"commentText\": \"Important region\",\n  \"tagsJson\": [\"diagram\"]\n}"
      },
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      },
      "parentId": "fld_highlights"
    },
    {
      "_id": "req_update_highlight",
      "_type": "request",
      "name": "Update Highlight",
      "method": "PUT",
      "url": "{{ _.baseUrl }}/api/highlights/{{ _.highlightId }}",
      "body": {
        "mimeType": "application/json",
        "text": "{\n  \"colorKey\": \"blue\",\n  \"commentText\": \"Updated comment!\",\n  \"tagsJson\": [\"study\", \"exam\"]\n}"
      },
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      },
      "parentId": "fld_highlights"
    },
    {
      "_id": "req_delete_highlight",
      "_type": "request",
      "name": "Delete Highlight",
      "method": "DELETE",
      "url": "{{ _.baseUrl }}/api/highlights/{{ _.highlightId }}",
      "headers": [],
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      },
      "parentId": "fld_highlights"
    },
    {
      "_id": "fld_files",
      "_type": "request_group",
      "name": "5. Files",
      "parentId": "wrk_cornell_reader"
    },
    {
      "_id": "req_file_url",
      "_type": "request",
      "name": "Get File View URL",
      "method": "GET",
      "url": "{{ _.baseUrl }}/api/files/{{ _.fileId }}/view-url",
      "headers": [],
      "authentication": {
        "type": "bearer",
        "token": "{{ _.token }}"
      },
      "parentId": "fld_files"
    }
  ]
}
